<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Nama Rak</th>
      <th>Nama Gudang</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < rak.length; i++){ %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= rak[i].name %></td>
          <td><%= rak[i].gudang?.name %></td>
          <td>
            <a href="/rak/edit/<%= rak[i]._id %>" class="btn btn-info btn-sm">
              <i class="fas fa-pencil-alt"></i>
              Ubah
            </a>
            <form style="display: inline-block;" action="/rak/delete/<%= rak[i]._id %>?_method=DELETE" method="POST">
              <button class="btn btn-danger btn-sm">
                <i class="fas fa-trash"></i>
                Hapus
              </button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>