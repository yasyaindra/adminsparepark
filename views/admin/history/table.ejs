<table id="example2" class="table table-bordered table-hover">
    <thead>
      <tr>
        <th>No.</th>
        <th>Nama Item</th>
        <th>Gudang</th>
        <th>Rak</th>
        <th>Nama Pengambil</th>
        <th>Quantity</th>
        <th>Tanggal Masuk</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < history.length; i++){ %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= history[i].kodeBarang%> - <%= history[i].name %></td>
          <td><%= namaGudang[i] %></td>
          <td><%= history[i].rak %></td>
          <td><%= history[i].namaPengambil %></td>
          <td><%= history[i].quantityDiambil %></td>
          <td><%= moment(history[i].createdAt).format("YYYY-MM-DD HH:mm"); %></td>
          <td><% if(!history[i].namaPengambil) { %><span class="badge badge-success">Ditambah</span><% } else { %><span class="badge badge-danger">Diambil</span><% } %></td>
        </tr>
      <% } %>
    </tbody>
  </table>