<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Nama Gudang</th>
      <th>Nama Rak</th>
      <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < gudang.length; i++){ %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= gudang[i].name %></td>
          <td>
            <% gudang[i].rak.forEach(nama => { %>
              <span class="badge badge-warning">
                <%= nama.name %> 
              </span>
            <% }) %>
          </td>
          <td>
            <form action="/gudang/delete/<%= gudang[i]._id %>?_method=DELETE" method="POST">
              <a href="/gudang/edit/<%= gudang[i]._id %>" class="btn btn-info btn-sm">
                <i class="fas fa-pencil-alt"></i>
                Ubah
              </a>
              <button class="btn btn-danger btn-sm">
                <i class="fas fa-trash"></i>
                Hapus
              </button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>